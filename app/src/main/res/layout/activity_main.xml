<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="id.ac.ui.cs.mobileprogramming.arifteguh.tugasuts.opengl.PermissionRequiredActivity">

    <id.ac.ui.cs.mobileprogramming.arifteguh.tugasuts.opengl.OGLView
        android:id="@+id/oglView"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />


    <TextView
        android:id="@+id/main_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="kosong"
        />

    <TextView
        android:id="@+id/TextViewTextPersonName3"
        android:layout_width="match_parent"
        android:layout_height="78dp"
        android:ems="10"
        android:layout_below="@id/main_text"
        android:inputType="textPersonName"
        android:text="@string/penjelasan1" />

    <TextView
        android:id="@+id/TextViewTextPersonName4"
        android:layout_width="match_parent"
        android:layout_height="90dp"
        android:layout_below="@id/TextViewTextPersonName3"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="@string/penjelasan2" />

    <TextView
        android:id="@+id/TextViewTextPersonName5"
        android:layout_width="match_parent"
        android:layout_height="90dp"
        android:layout_below="@id/TextViewTextPersonName4"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="@string/penjelasan3" />

    <TextView
        android:id="@+id/TextViewTextPersonName6"
        android:layout_width="match_parent"
        android:layout_height="114dp"
        android:layout_below="@id/TextViewTextPersonName5"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="@string/penjelasan4" />



</RelativeLayout>
